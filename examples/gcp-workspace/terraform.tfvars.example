### General
prefix = "myproject"

### Databricks
databricks_account_id = "12345678-1234-1234-1234-123456789012"
databricks_client_secret = "your-client-secret"
databricks_client_id = "12345678-1234-1234-1234-123456789012"
workspace_name = "my-workspace"
network_config_name = "my-network-config"
private_access_setting_name = "my-private-access"
pricing_tier = "PREMIUM"

### Google
google_project = "my-gcp-project"
region = "us-central1"
google_service_account = "sa-name@my-gcp-project.iam.gserviceaccount.com"

### Network Names
# If create_vpc = true, these will be used to name the new resources
# If create_vpc = false, these should be the names of existing resources
create_vpc = true
google_vpc_name = "my-vpc"
google_subnet_name = "my-subnet"
google_subnet_pods_name = "my-pods-subnet"
google_subnet_svc_name = "my-services-subnet"
google_router_name = "my-router"
google_nat_name = "my-nat"

### Network CIDRs
# Only used if create_vpc = true
google_vpc_cidr = "10.0.0.0/16"
google_vpc_cidr_newbits = 8
# If null, these will be calculated automatically
google_subnet_ip_cidr = null
google_pods_ip_cidr = null
google_svc_ip_cidr = null

### Private Service Connect
enable_psc = false
enable_dataplane_relay_psc = false
enable_rest_api_psc = false
google_dataplane_relay_endpoint_vpc_name = "my-dataplane-relay-vpc"
google_dataplane_relay_endpoint_psc_name = "my-dataplane-relay-psc"
google_dataplane_relay_endpoint_ip_name = "my-dataplane-relay-ip"
google_rest_api_endpoint_vpc_name = "my-rest-api-vpc"
google_rest_api_endpoint_psc_name = "my-rest-api-psc"
google_rest_api_endpoint_ip_name = "my-rest-api-ip"
